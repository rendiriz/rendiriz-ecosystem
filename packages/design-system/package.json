    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "^10.0.0",
    "stencil-tailwind-plugin": "^1.3.0",
    "tailwindcss": "^3.0.23",
    "tsconfig": "*"
  },
  "license": "MIT"
